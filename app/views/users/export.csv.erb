<%- headers = ['Name', 'Email', 'Created User', 'Created Date', "Updated Date"] -%>
<%= CSV.generate_line headers, :row_sep => ?\t, :quote_char => ?\ %>
<%- @users.each do |user| -%>
  <%= CSV.generate_line([
      user.name, 
      user.email,
      user.created_user == 1 ? 'Admin' : 'User',
      user.created_at.strftime('%Y/%m/%d'), 
      user.updated_at.strftime('%Y/%m/%d')
    ]).html_safe -%>
<%- end -%>